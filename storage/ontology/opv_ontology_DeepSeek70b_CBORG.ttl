@prefix opv: <http://opv-kg.org/ontology/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

opv:Analysis_Target a owl:Class ;
    rdfs:label "Analysis Target" ;
    rdfs:comment "A property indicating the presence of oxidized P3HT (P3HT+) and PSS in the interface layer, used as a target for analysis." ;
    rdfs:subClassOf opv:property .

opv:BHJ_layer a owl:Class ;
    rdfs:label "BHJ layer" ;
    rdfs:comment "Active layer in organic photovoltaic devices where charge separation occurs, typically composed of a bulk heterojunction of donor and acceptor materials." ;
    rdfs:subClassOf opv:material .

opv:BulkHeterojunction a owl:Class ;
    rdfs:label "BulkHeterojunction" ;
    opv:contains_material opv:P3HT,
        opv:PCBM ;
    rdfs:comment "A mixed layer of donor and acceptor materials with a high interfacial area" ;
    rdfs:subClassOf opv:DeviceArchitecture .

opv:Contact_Angle_Measurements a owl:Class ;
    rdfs:label "Contact Angle Measurements" ;
    rdfs:comment "A technique used to analyze the physical mixing behavior of P3HT with PEDOT:PSS upon heating." ;
    rdfs:subClassOf opv:processingmethod .

opv:Crystallinity a owl:Class ;
    rdfs:label "Crystallinity" ;
    rdfs:comment "The degree of crystalline order in a material, which increases after curing." ;
    rdfs:subClassOf opv:morphologicalproperty .

<http://opv-kg.org/ontology/Current_Density–Voltage_(J–V)_Measurements> a owl:Class ;
    rdfs:label "Current Density–Voltage (J–V) Measurements" ;
    rdfs:comment "A measurement technique used to evaluate the open-circuit voltage (Voc) of OPV devices by analyzing current density versus voltage characteristics." ;
    rdfs:subClassOf opv:experiment .

opv:Dark_Current_Analysis a owl:Class ;
    rdfs:label "Dark Current Analysis" ;
    rdfs:comment "Analysis of dark current to evaluate interface effects on device performance and characteristics." ;
    rdfs:subClassOf opv:Current-Voltage_\(I-V\)_Measurements .

opv:Dark_current_limitation a owl:Class ;
    rdfs:label "Dark current limitation" ;
    rdfs:comment "A limitation caused by interface mixing that impacts open-circuit voltage and compensation voltage in organic photovoltaics." ;
    rdfs:subClassOf opv:morphologicalproperty .

opv:Domain_size a owl:Class ;
    rdfs:label "Domain size" ;
    rdfs:comment "The size of crystalline domains in the active layer, which increases after curing during processing." ;
    rdfs:subClassOf opv:morphologicalproperty .

opv:Duration a owl:Class ;
    rdfs:label "Duration" ;
    rdfs:comment "Time duration for thermal curing process." ;
    rdfs:subClassOf opv:thermal_treatment_\(annealing\) .

opv:Experiment a owl:Class .

opv:FTIR_Spectroscopy a owl:Class ;
    rdfs:label "FTIR Spectroscopy" ;
    rdfs:comment "FTIR spectroscopy confirms the chemical reaction between P3HT and HPSS by analyzing their molecular interactions." ;
    rdfs:subClassOf opv:experiment .

opv:Fill_Factor_\(FF\) a owl:Class ;
    rdfs:label "Fill Factor (FF)" ;
    rdfs:comment "A parameter derived from current-voltage (I-V) curves to assess the efficiency of photovoltaic devices." ;
    rdfs:subClassOf opv:property .

opv:Hole_mobility a owl:Class ;
    rdfs:label "Hole mobility" ;
    rdfs:comment "Hole mobility refers to the measure of how easily holes move through a material, which can be enhanced through curing processes." ;
    rdfs:subClassOf opv:property .

opv:I-V_Curve_Analysis a owl:Class ;
    rdfs:label "I-V Curve Analysis" ;
    rdfs:comment "Analyzes I-V curves to extract key photovoltaic parameters." ;
    rdfs:subClassOf opv:analysis_target .

opv:ITO_substrate a owl:Class ;
    rdfs:label "ITO substrate" ;
    rdfs:comment "transparent electrode" ;
    rdfs:subClassOf opv:material .

opv:Illumination a owl:Class ;
    rdfs:label "Illumination" ;
    rdfs:comment "Standard solar illumination condition used for measuring power conversion efficiency (PCE) in OPV devices." ;
    rdfs:subClassOf opv:experiment .

opv:Kelvin_Force_Probe a owl:Class ;
    rdfs:label "Kelvin Force Probe" ;
    rdfs:comment "A technique used to measure surface potential changes at the anode/BHJ interface." ;
    rdfs:subClassOf opv:experiment .

<http://opv-kg.org/ontology/N/A> a owl:Class ;
    rdfs:label "N/A" ;
    rdfs:comment "A placeholder indicating missing or unspecified information in the ontology." ;
    rdfs:subClassOf opv:property .

opv:Neutron_Reflectometry_\(NR\) a owl:Class ;
    rdfs:label "Neutron Reflectometry (NR)" ;
    rdfs:comment "Analyzes structural changes at the P3HT/PEDOT:PSS interface using neutron reflectometry." ;
    rdfs:subClassOf opv:experiment .

opv:NiO a owl:Class ;
    rdfs:label "NiO" ;
    rdfs:comment "An inorganic material used as an electrode in organic photovoltaic devices." ;
    rdfs:subClassOf opv:electrode_material .

opv:Open-Circuit_Voltage_\(Voc\) a owl:Class ;
    rdfs:label "Open-Circuit Voltage (Voc)" ;
    rdfs:comment "A parameter derived from I-V curves, representing the maximum voltage at open circuit, used to assess interface effects in OPV devices." ;
    rdfs:subClassOf opv:i-v_curve_analysis .

opv:Oxidation_state a owl:Class ;
    rdfs:label "Oxidation state" ;
    rdfs:comment "The oxidized state of a material resulting from heat treatment processes." ;
    rdfs:subClassOf opv:property .

<http://opv-kg.org/ontology/P3HT:PCBM_blend> a owl:Class ;
    rdfs:label "P3HT:PCBM blend" ;
    rdfs:comment "A blend of poly(3-hexylthiophene) (P3HT) and [6,6]-phenyl-C61-butyric acid methyl ester (PCBM) used as the active layer in bulk heterojunction (BHJ) organic photovoltaic devices." ;
    rdfs:subClassOf opv:material .

<http://opv-kg.org/ontology/P3HT_and_PSS_(oxidized_P3HT+)> a owl:Class ;
    rdfs:label "P3HT and PSS (oxidized P3HT+)" ;
    rdfs:comment "A composite material consisting of poly(3-hexylthiophene) (P3HT) and poly(styrenesulfonate) (PSS) in an oxidized state (P3HT+), utilized in the mixed interface layer of organic photovoltaic devices." ;
    rdfs:subClassOf opv:material .

<http://opv-kg.org/ontology/PEDOT:PSS> a owl:Class ;
    rdfs:label "PEDOT:PSS" ;
    rdfs:comment "A conductive polymer used as a hole transport layer in organic photovoltaics." ;
    rdfs:subClassOf opv:polymer .

<http://opv-kg.org/ontology/PEDOT:PSS_layer> a owl:Class ;
    rdfs:label "PEDOT:PSS layer" ;
    rdfs:comment "polymer anode" ;
    rdfs:subClassOf opv:material .

opv:ProcessingMethod a owl:Class ;
    rdfs:label "ProcessingMethod" ;
    opv:affects opv:DomainOrientation ;
    rdfs:comment "A technique used to create or modify a material or device" .

opv:Solvent a owl:Class ;
    rdfs:label "Solvent" ;
    rdfs:comment "A substance used to dissolve or remove materials in OPV processing, such as removing soluble P3HT from interfaces." ;
    rdfs:subClassOf opv:Material .

opv:Solvent_Soaking a owl:Class ;
    rdfs:label "Solvent Soaking" ;
    rdfs:comment "A processing method that enhances bulk heterojunction (BHJ) morphology through solvent-induced phase separation." ;
    rdfs:subClassOf opv:processingmethod .

opv:Solvent_Type a owl:Class ;
    rdfs:label "Solvent Type" ;
    rdfs:comment "The type of solvent used in the processing of the bulk heterojunction layer." ;
    rdfs:subClassOf opv:Material .

opv:Spin_Coating a owl:Class ;
    rdfs:label "Spin Coating" ;
    rdfs:comment "A deposition technique used to form thin films of materials like PEDOT:PSS and P3HT:PCBM blends by spinning a solution onto a substrate." ;
    rdfs:subClassOf opv:processingmethod .

opv:TFB a owl:Class ;
    rdfs:label "TFB" ;
    rdfs:comment "A polymer used in organic photovoltaics" ;
    rdfs:subClassOf opv:polymer .

opv:TPDSi2 a owl:Class ;
    rdfs:label "TPDSi2" ;
    rdfs:comment "A small molecule used in organic photovoltaics" ;
    rdfs:subClassOf opv:material .

opv:Temperature a owl:Class ;
    rdfs:label "Temperature" ;
    rdfs:comment "Heat treatment applied to cure the bulk heterojunction (BHJ) layer, enhancing its morphology." ;
    rdfs:subClassOf opv:thermal_treatment_\(annealing\) .

opv:Thermal_Treatment a owl:Class ;
    rdfs:label "Thermal Treatment" ;
    rdfs:comment "A heating process applied to modify the interface between the anode and bulk heterojunction layer in organic photovoltaic devices." ;
    rdfs:subClassOf opv:processingmethod .

opv:Thermal_Treatment_\(Annealing\) a owl:Class ;
    rdfs:label "Thermal Treatment (Annealing)" ;
    rdfs:comment "A thermal treatment process applied to the bulk-heterojunction layer to enhance crystallinity and domain size." ;
    rdfs:subClassOf opv:processingmethod .

opv:UV-Vis_Spectroscopy a owl:Class ;
    rdfs:label "UV-Vis Spectroscopy" ;
    rdfs:comment "A spectroscopic technique used to measure the optical absorption properties of polymer layers in OPV devices." ;
    rdfs:subClassOf opv:experiment .

<http://opv-kg.org/ontology/UV/VIS/NIR_Spectroscopy> a owl:Class ;
    rdfs:label "UV/VIS/NIR Spectroscopy" ;
    rdfs:comment "A method to monitor the oxidation of P3HT to P3HT+ in the mixed layer of organic photovoltaic devices." ;
    rdfs:subClassOf opv:uv-vis_spectroscopy .

opv:V2O5 a owl:Class ;
    rdfs:label "V2O5" ;
    rdfs:comment "An inorganic material used in organic photovoltaics." ;
    rdfs:subClassOf opv:material .

opv:Voltage_Range a owl:Class ;
    rdfs:label "Voltage Range" ;
    rdfs:comment "The range of voltages applied during dark current measurements to analyze device performance." ;
    rdfs:subClassOf opv:dark_current_analysis .

opv:Washing_with_Chlorobenzene_\(CB\) a owl:Class ;
    rdfs:label "Washing with Chlorobenzene (CB)" ;
    rdfs:comment "A post-treatment process using chlorobenzene to isolate the mixed interface layer in OPV devices." ;
    rdfs:subClassOf opv:processingmethod .

opv:Wavelength_Range a owl:Class ;
    rdfs:label "Wavelength Range" ;
    rdfs:comment "A measure of the range of wavelengths absorbed by materials, used to analyze optical properties of P3HT and PEDOT:PSS layers." ;
    rdfs:subClassOf opv:property .

opv:Work_function a owl:Class ;
    rdfs:label "Work function" ;
    rdfs:comment "The minimum energy required to remove an electron from a material's surface, critical for charge transfer in OPV devices." ;
    rdfs:subClassOf opv:property .

opv:X-ray_Absorption_Near_Edge_Structure_\(XANES\) a owl:Class ;
    rdfs:label "X-ray Absorption Near Edge Structure (XANES)" ;
    rdfs:comment "A spectroscopic technique used to analyze the chemical composition and electronic structure at interfaces in OPV devices." ;
    rdfs:subClassOf opv:experiment .

opv:X-ray_Photoelectron_Spectroscopy_\(XPS\) a owl:Class ;
    rdfs:label "X-ray Photoelectron Spectroscopy (XPS)" ;
    rdfs:comment "A characterization technique used to analyze the chemical composition and oxidation states at the P3HT/PEDOT:PSS interface." ;
    rdfs:subClassOf opv:experiment .

opv:XPS_Data_Analysis a owl:Class ;
    rdfs:label "XPS Data Analysis" ;
    rdfs:comment "Interprets XPS spectra to determine chemical composition and oxidation states of materials." ;
    rdfs:subClassOf opv:x-ray_photoelectron_spectroscopy_\(xps\) .

opv:affects a owl:ObjectProperty ;
    rdfs:comment "Processing methods can influence the orientation of P3HT domains" .

<http://opv-kg.org/ontology/aluminum_(assumed,_not_explicitly_mentioned)> a owl:Class ;
    rdfs:label "aluminum (assumed, not explicitly mentioned)" ;
    rdfs:comment "A material used in the cathode layer of organic photovoltaic devices" ;
    rdfs:subClassOf opv:cathode .

opv:contains_material a owl:ObjectProperty ;
    rdfs:comment "P3HT is commonly used as the donor material in bulk heterojunction OPVs",
        "PCBM is commonly used as the acceptor material in bulk heterojunction OPVs" .

opv:domain_formation a owl:Class ;
    rdfs:label "domain formation" ;
    rdfs:comment "The process of forming lateral PEDOT-rich domains through thermal treatment." ;
    rdfs:subClassOf opv:morphologicalproperty .

opv:has_property a owl:ObjectProperty ;
    rdfs:comment "P3HT forms domains with specific orientations that affect device performance" .

opv:hole_selectivity a owl:Class ;
    rdfs:label "hole selectivity" ;
    rdfs:comment "The ability of a material or layer to preferentially conduct holes (positive charge carriers) while minimizing electron transport, critical for efficient charge separation in OPV devices. Example: TFB blend exhibits higher hole selectivity compared to PEDOT:PSS." ;
    rdfs:subClassOf opv:Property .

opv:indium-tin_oxide a owl:Class ;
    rdfs:label "indium-tin oxide" ;
    rdfs:comment "A material used in the ITO substrate layer of organic photovoltaic devices" ;
    rdfs:subClassOf opv:ito_substrate .

opv:metallic_behavior a owl:Class ;
    rdfs:label "metallic behavior" ;
    rdfs:comment "Characterizes materials exhibiting metallic behavior, defined by a continuous density of states at the Fermi level, enabling high electrical conductivity." ;
    rdfs:subClassOf opv:conductivity .

opv:mixed_interface_layer a owl:Class ;
    rdfs:label "mixed interface layer" ;
    rdfs:comment "The interface layer between PEDOT:PSS and the bulk heterojunction (BHJ) layer in an OPV device." ;
    rdfs:subClassOf opv:devicearchitecture .

opv:mixing_behavior a owl:Class ;
    rdfs:label "mixing behavior" ;
    rdfs:comment "The tendency of a material to physically mix with PEDOT:PSS when heated." ;
    rdfs:subClassOf opv:morphologicalproperty .

opv:optical_transparency a owl:Class ;
    rdfs:label "optical transparency" ;
    rdfs:comment "Desired property for hole-transporting layer, indicating the material's ability to transmit light effectively." ;
    rdfs:subClassOf opv:property .

opv:organic_photovoltaic_\(OPV\)_device a owl:Class ;
    rdfs:label "organic photovoltaic (OPV) device" ;
    rdfs:comment "A type of organic photovoltaic device characterized by a bulk-heterojunction (BHJ) structure with a mixed interface layer." ;
    rdfs:subClassOf opv:device .

opv:oxidation a owl:Class ;
    rdfs:label "oxidation" ;
    rdfs:comment "Oxidation of P3HT to P3HT+ via reaction with HPSS, enhancing hole transport selectivity in OPV devices." ;
    rdfs:subClassOf opv:processingmethod .

<http://opv-kg.org/ontology/poly(3,4-ethylenedioxythiophene):poly(styrenesulfonate)> a owl:Class ;
    rdfs:label "poly(3,4-ethylenedioxythiophene):poly(styrenesulfonate)" ;
    rdfs:comment "A polymer blend used as a conductive layer in organic photovoltaic devices." ;
    rdfs:subClassOf <http://opv-kg.org/ontology/pedot:pss> .

opv:polymer_solar_cell a owl:Class ;
    rdfs:label "polymer solar cell" ;
    rdfs:comment "A polymer solar cell with a typical architecture of ITO/PEDOT:PSS/active layer (P3HT:PCBM)/Ca/Ag, representing a specific type of organic photovoltaic device." ;
    rdfs:subClassOf opv:organic_photovoltaic_\(opv\)_device .

opv:vertical_segregation a owl:Class ;
    rdfs:label "vertical segregation" ;
    rdfs:comment "Segregation of poly(styrenesulfonate) (PSS) to the top surface during spin-coating in air." ;
    rdfs:subClassOf opv:morphologicalproperty .

opv:θc a owl:Class ;
    rdfs:label "θc" ;
    rdfs:comment "Surface energy characterization of the P3HT/PEDOT:PSS interface" ;
    rdfs:subClassOf opv:MorphologicalProperty .

opv:Current-Voltage_\(I-V\)_Measurements a owl:Class ;
    rdfs:label "Current-Voltage (I-V) Measurements" ;
    rdfs:comment "A method to measure device performance metrics (PCE, Jsc, Voc, FF) under simulated sunlight." ;
    rdfs:subClassOf opv:experiment .

opv:Device a owl:Class .

opv:DeviceArchitecture a owl:Class ;
    rdfs:subClassOf opv:Device .

opv:P3HT a owl:Class ;
    rdfs:label "P3HT" ;
    opv:has_property opv:DomainOrientation ;
    rdfs:comment "Poly(3-hexylthiophene), a semiconducting polymer commonly used in OPVs" ;
    rdfs:subClassOf opv:Polymer .

opv:PCBM a owl:Class ;
    rdfs:label "PCBM" ;
    rdfs:comment "Phenyl-C61-butyric acid methyl ester, a fullerene derivative commonly used as an electron acceptor in OPVs" ;
    rdfs:subClassOf opv:Material .

opv:Polymer a owl:Class ;
    rdfs:label "Polymer" ;
    rdfs:comment "A large molecule composed of repeating structural units" ;
    rdfs:subClassOf opv:Material .

opv:cathode a owl:Class ;
    rdfs:label "cathode" ;
    rdfs:comment "Electron-collecting electrode in an organic photovoltaic device." ;
    rdfs:subClassOf opv:devicearchitecture .

opv:conductivity a owl:Class ;
    rdfs:label "conductivity" ;
    rdfs:comment "Conductivity refers to the ability of PEDOT-rich domains to conduct electricity, which increases upon thermal treatment." ;
    rdfs:subClassOf opv:property .

opv:electrode_material a owl:Class ;
    rdfs:label "electrode material" ;
    rdfs:comment "A material used as an electrode in OPV devices, investigated as a replacement for PEDOT:PSS." ;
    rdfs:subClassOf opv:material .

opv:DomainOrientation a owl:Class ;
    rdfs:label "DomainOrientation" ;
    rdfs:comment "The alignment direction of polymer domains within a film" ;
    rdfs:subClassOf opv:MorphologicalProperty .

opv:MorphologicalProperty a owl:Class ;
    rdfs:subClassOf opv:Property .

opv:Property a owl:Class .

opv:Material a owl:Class .

