<?xml version="1.0" encoding="utf-8"?>
<rdf:RDF
   xmlns:opv="http://opv-kg.org/ontology/"
   xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
   xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/Solvent">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>Solvent</rdfs:label>
    <rdfs:comment>A substance used to dissolve or remove materials in OPV processing, such as removing soluble P3HT from interfaces.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://opv-kg.org/ontology/Material"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/aluminum_(assumed,_not_explicitly_mentioned)">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>aluminum (assumed, not explicitly mentioned)</rdfs:label>
    <rdfs:comment>A material used in the cathode layer of organic photovoltaic devices</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://opv-kg.org/ontology/cathode"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/Current_Density–Voltage_(J–V)_Measurements">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>Current Density–Voltage (J–V) Measurements</rdfs:label>
    <rdfs:comment>A measurement technique used to evaluate the open-circuit voltage (Voc) of OPV devices by analyzing current density versus voltage characteristics.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://opv-kg.org/ontology/experiment"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/contains_material">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:comment>P3HT is commonly used as the donor material in bulk heterojunction OPVs</rdfs:comment>
    <rdfs:comment>PCBM is commonly used as the acceptor material in bulk heterojunction OPVs</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/optical_transparency">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>optical transparency</rdfs:label>
    <rdfs:comment>Desired property for hole-transporting layer, indicating the material's ability to transmit light effectively.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://opv-kg.org/ontology/property"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/conductivity">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>conductivity</rdfs:label>
    <rdfs:comment>Conductivity refers to the ability of PEDOT-rich domains to conduct electricity, which increases upon thermal treatment.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://opv-kg.org/ontology/property"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/PEDOT:PSS">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>PEDOT:PSS</rdfs:label>
    <rdfs:comment>A conductive polymer used as a hole transport layer in organic photovoltaics.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://opv-kg.org/ontology/polymer"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/Thermal_Treatment">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>Thermal Treatment</rdfs:label>
    <rdfs:comment>A heating process applied to modify the interface between the anode and bulk heterojunction layer in organic photovoltaic devices.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://opv-kg.org/ontology/processingmethod"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/mixing_behavior">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>mixing behavior</rdfs:label>
    <rdfs:comment>The tendency of a material to physically mix with PEDOT:PSS when heated.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://opv-kg.org/ontology/morphologicalproperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/Duration">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>Duration</rdfs:label>
    <rdfs:comment>Time duration for thermal curing process.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://opv-kg.org/ontology/thermal_treatment_(annealing)"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/P3HT:PCBM_blend">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>P3HT:PCBM blend</rdfs:label>
    <rdfs:comment>A blend of poly(3-hexylthiophene) (P3HT) and [6,6]-phenyl-C61-butyric acid methyl ester (PCBM) used as the active layer in bulk heterojunction (BHJ) organic photovoltaic devices.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://opv-kg.org/ontology/material"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/Dark_Current_Analysis">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>Dark Current Analysis</rdfs:label>
    <rdfs:comment>Analysis of dark current to evaluate interface effects on device performance and characteristics.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://opv-kg.org/ontology/Current-Voltage_(I-V)_Measurements"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/Kelvin_Force_Probe">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>Kelvin Force Probe</rdfs:label>
    <rdfs:comment>A technique used to measure surface potential changes at the anode/BHJ interface.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://opv-kg.org/ontology/experiment"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/hole_selectivity">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>hole selectivity</rdfs:label>
    <rdfs:comment>The ability of a material or layer to preferentially conduct holes (positive charge carriers) while minimizing electron transport, critical for efficient charge separation in OPV devices. Example: TFB blend exhibits higher hole selectivity compared to PEDOT:PSS.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://opv-kg.org/ontology/Property"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/domain_formation">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>domain formation</rdfs:label>
    <rdfs:comment>The process of forming lateral PEDOT-rich domains through thermal treatment.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://opv-kg.org/ontology/morphologicalproperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/XPS_Data_Analysis">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>XPS Data Analysis</rdfs:label>
    <rdfs:comment>Interprets XPS spectra to determine chemical composition and oxidation states of materials.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://opv-kg.org/ontology/x-ray_photoelectron_spectroscopy_(xps)"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/cathode">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>cathode</rdfs:label>
    <rdfs:comment>Electron-collecting electrode in an organic photovoltaic device.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://opv-kg.org/ontology/devicearchitecture"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/anode">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>anode</rdfs:label>
    <rdfs:comment>None</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/ProcessingMethod">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>ProcessingMethod</rdfs:label>
    <rdfs:comment>A technique used to create or modify a material or device</rdfs:comment>
    <opv:affects rdf:resource="http://opv-kg.org/ontology/DomainOrientation"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/BHJ_layer">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>BHJ layer</rdfs:label>
    <rdfs:comment>Active layer in organic photovoltaic devices where charge separation occurs, typically composed of a bulk heterojunction of donor and acceptor materials.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://opv-kg.org/ontology/material"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/Wavelength_Range">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>Wavelength Range</rdfs:label>
    <rdfs:comment>A measure of the range of wavelengths absorbed by materials, used to analyze optical properties of P3HT and PEDOT:PSS layers.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://opv-kg.org/ontology/property"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/θc">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>θc</rdfs:label>
    <rdfs:comment>Surface energy characterization of the P3HT/PEDOT:PSS interface</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://opv-kg.org/ontology/MorphologicalProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/affects">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:comment>Processing methods can influence the orientation of P3HT domains</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/DeviceArchitecture">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://opv-kg.org/ontology/Device"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/Thermal_Treatment_(Annealing)">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>Thermal Treatment (Annealing)</rdfs:label>
    <rdfs:comment>A thermal treatment process applied to the bulk-heterojunction layer to enhance crystallinity and domain size.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://opv-kg.org/ontology/processingmethod"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/Spin_Coating">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>Spin Coating</rdfs:label>
    <rdfs:comment>A deposition technique used to form thin films of materials like PEDOT:PSS and P3HT:PCBM blends by spinning a solution onto a substrate.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://opv-kg.org/ontology/processingmethod"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/Crystallinity">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>Crystallinity</rdfs:label>
    <rdfs:comment>The degree of crystalline order in a material, which increases after curing.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://opv-kg.org/ontology/morphologicalproperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/Domain_size">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>Domain size</rdfs:label>
    <rdfs:comment>The size of crystalline domains in the active layer, which increases after curing during processing.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://opv-kg.org/ontology/morphologicalproperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/X-ray_Photoelectron_Spectroscopy_(XPS)">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>X-ray Photoelectron Spectroscopy (XPS)</rdfs:label>
    <rdfs:comment>A characterization technique used to analyze the chemical composition and oxidation states at the P3HT/PEDOT:PSS interface.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://opv-kg.org/ontology/experiment"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/Fill_Factor_(FF)">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>Fill Factor (FF)</rdfs:label>
    <rdfs:comment>A parameter derived from current-voltage (I-V) curves to assess the efficiency of photovoltaic devices.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://opv-kg.org/ontology/property"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/NiO">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>NiO</rdfs:label>
    <rdfs:comment>An inorganic material used as an electrode in organic photovoltaic devices.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://opv-kg.org/ontology/electrode_material"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/I-V_Curve_Analysis">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>I-V Curve Analysis</rdfs:label>
    <rdfs:comment>Analyzes I-V curves to extract key photovoltaic parameters.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://opv-kg.org/ontology/analysis_target"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/N/A">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>N/A</rdfs:label>
    <rdfs:comment>A placeholder indicating missing or unspecified information in the ontology.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://opv-kg.org/ontology/property"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/Analysis_Target">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>Analysis Target</rdfs:label>
    <rdfs:comment>A property indicating the presence of oxidized P3HT (P3HT+) and PSS in the interface layer, used as a target for analysis.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://opv-kg.org/ontology/property"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/mixed_interface_layer">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>mixed interface layer</rdfs:label>
    <rdfs:comment>The interface layer between PEDOT:PSS and the bulk heterojunction (BHJ) layer in an OPV device.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://opv-kg.org/ontology/devicearchitecture"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/V2O5">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>V2O5</rdfs:label>
    <rdfs:comment>An inorganic material used in organic photovoltaics.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://opv-kg.org/ontology/material"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/Neutron_Reflectometry_(NR)">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>Neutron Reflectometry (NR)</rdfs:label>
    <rdfs:comment>Analyzes structural changes at the P3HT/PEDOT:PSS interface using neutron reflectometry.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://opv-kg.org/ontology/experiment"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/X-ray_Absorption_Near_Edge_Structure_(XANES)">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>X-ray Absorption Near Edge Structure (XANES)</rdfs:label>
    <rdfs:comment>A spectroscopic technique used to analyze the chemical composition and electronic structure at interfaces in OPV devices.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://opv-kg.org/ontology/experiment"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/Experiment">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/Contact_Angle_Measurements">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>Contact Angle Measurements</rdfs:label>
    <rdfs:comment>A technique used to analyze the physical mixing behavior of P3HT with PEDOT:PSS upon heating.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://opv-kg.org/ontology/processingmethod"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/indium-tin_oxide">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>indium-tin oxide</rdfs:label>
    <rdfs:comment>A material used in the ITO substrate layer of organic photovoltaic devices</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://opv-kg.org/ontology/ito_substrate"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/Voltage_Range">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>Voltage Range</rdfs:label>
    <rdfs:comment>The range of voltages applied during dark current measurements to analyze device performance.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://opv-kg.org/ontology/dark_current_analysis"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/Dark_current_limitation">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>Dark current limitation</rdfs:label>
    <rdfs:comment>A limitation caused by interface mixing that impacts open-circuit voltage and compensation voltage in organic photovoltaics.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://opv-kg.org/ontology/morphologicalproperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/Hole_mobility">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>Hole mobility</rdfs:label>
    <rdfs:comment>Hole mobility refers to the measure of how easily holes move through a material, which can be enhanced through curing processes.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://opv-kg.org/ontology/property"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/Polymer">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://opv-kg.org/ontology/Material"/>
    <rdfs:label>Polymer</rdfs:label>
    <rdfs:comment>A large molecule composed of repeating structural units</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/PEDOT:PSS_layer">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>PEDOT:PSS layer</rdfs:label>
    <rdfs:comment>polymer anode</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://opv-kg.org/ontology/material"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/vertical_segregation">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>vertical segregation</rdfs:label>
    <rdfs:comment>Segregation of poly(styrenesulfonate) (PSS) to the top surface during spin-coating in air.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://opv-kg.org/ontology/morphologicalproperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/DomainOrientation">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>DomainOrientation</rdfs:label>
    <rdfs:comment>The alignment direction of polymer domains within a film</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://opv-kg.org/ontology/MorphologicalProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/electrode_material">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>electrode material</rdfs:label>
    <rdfs:comment>A material used as an electrode in OPV devices, investigated as a replacement for PEDOT:PSS.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://opv-kg.org/ontology/material"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/Temperature">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>Temperature</rdfs:label>
    <rdfs:comment>Heat treatment applied to cure the bulk heterojunction (BHJ) layer, enhancing its morphology.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://opv-kg.org/ontology/thermal_treatment_(annealing)"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/BulkHeterojunction">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>BulkHeterojunction</rdfs:label>
    <rdfs:comment>A mixed layer of donor and acceptor materials with a high interfacial area</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://opv-kg.org/ontology/DeviceArchitecture"/>
    <opv:contains_material rdf:resource="http://opv-kg.org/ontology/P3HT"/>
    <opv:contains_material rdf:resource="http://opv-kg.org/ontology/PCBM"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/organic_photovoltaic_(OPV)_device">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>organic photovoltaic (OPV) device</rdfs:label>
    <rdfs:comment>A type of organic photovoltaic device characterized by a bulk-heterojunction (BHJ) structure with a mixed interface layer.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://opv-kg.org/ontology/device"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/UV-Vis_Spectroscopy">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>UV-Vis Spectroscopy</rdfs:label>
    <rdfs:comment>A spectroscopic technique used to measure the optical absorption properties of polymer layers in OPV devices.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://opv-kg.org/ontology/experiment"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/TFB">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>TFB</rdfs:label>
    <rdfs:comment>A polymer used in organic photovoltaics</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://opv-kg.org/ontology/polymer"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/P3HT">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>P3HT</rdfs:label>
    <rdfs:comment>Poly(3-hexylthiophene), a semiconducting polymer commonly used in OPVs</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://opv-kg.org/ontology/Polymer"/>
    <opv:has_property rdf:resource="http://opv-kg.org/ontology/DomainOrientation"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/PCBM">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>PCBM</rdfs:label>
    <rdfs:comment>Phenyl-C61-butyric acid methyl ester, a fullerene derivative commonly used as an electron acceptor in OPVs</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://opv-kg.org/ontology/Material"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/oxidation">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>oxidation</rdfs:label>
    <rdfs:comment>Oxidation of P3HT to P3HT+ via reaction with HPSS, enhancing hole transport selectivity in OPV devices.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://opv-kg.org/ontology/processingmethod"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/polymer_solar_cell">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>polymer solar cell</rdfs:label>
    <rdfs:comment>A polymer solar cell with a typical architecture of ITO/PEDOT:PSS/active layer (P3HT:PCBM)/Ca/Ag, representing a specific type of organic photovoltaic device.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://opv-kg.org/ontology/organic_photovoltaic_(opv)_device"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/Work_function">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>Work function</rdfs:label>
    <rdfs:comment>The minimum energy required to remove an electron from a material's surface, critical for charge transfer in OPV devices.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://opv-kg.org/ontology/property"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/TPDSi2">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>TPDSi2</rdfs:label>
    <rdfs:comment>A small molecule used in organic photovoltaics</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://opv-kg.org/ontology/material"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/Solvent_Soaking">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>Solvent Soaking</rdfs:label>
    <rdfs:comment>A processing method that enhances bulk heterojunction (BHJ) morphology through solvent-induced phase separation.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://opv-kg.org/ontology/processingmethod"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/has_property">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:comment>P3HT forms domains with specific orientations that affect device performance</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/Washing_with_Chlorobenzene_(CB)">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>Washing with Chlorobenzene (CB)</rdfs:label>
    <rdfs:comment>A post-treatment process using chlorobenzene to isolate the mixed interface layer in OPV devices.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://opv-kg.org/ontology/processingmethod"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/Oxidation_state">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>Oxidation state</rdfs:label>
    <rdfs:comment>The oxidized state of a material resulting from heat treatment processes.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://opv-kg.org/ontology/property"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/UV/VIS/NIR_Spectroscopy">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>UV/VIS/NIR Spectroscopy</rdfs:label>
    <rdfs:comment>A method to monitor the oxidation of P3HT to P3HT+ in the mixed layer of organic photovoltaic devices.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://opv-kg.org/ontology/uv-vis_spectroscopy"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/FTIR_Spectroscopy">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>FTIR Spectroscopy</rdfs:label>
    <rdfs:comment>FTIR spectroscopy confirms the chemical reaction between P3HT and HPSS by analyzing their molecular interactions.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://opv-kg.org/ontology/experiment"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/Illumination">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>Illumination</rdfs:label>
    <rdfs:comment>Standard solar illumination condition used for measuring power conversion efficiency (PCE) in OPV devices.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://opv-kg.org/ontology/experiment"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/Solvent_Type">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>Solvent Type</rdfs:label>
    <rdfs:comment>The type of solvent used in the processing of the bulk heterojunction layer.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://opv-kg.org/ontology/Material"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/metallic_behavior">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>metallic behavior</rdfs:label>
    <rdfs:comment>Characterizes materials exhibiting metallic behavior, defined by a continuous density of states at the Fermi level, enabling high electrical conductivity.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://opv-kg.org/ontology/conductivity"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/poly(3,4-ethylenedioxythiophene):poly(styrenesulfonate)">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>poly(3,4-ethylenedioxythiophene):poly(styrenesulfonate)</rdfs:label>
    <rdfs:comment>A polymer blend used as a conductive layer in organic photovoltaic devices.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://opv-kg.org/ontology/pedot:pss"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/Current-Voltage_(I-V)_Measurements">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>Current-Voltage (I-V) Measurements</rdfs:label>
    <rdfs:comment>A method to measure device performance metrics (PCE, Jsc, Voc, FF) under simulated sunlight.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://opv-kg.org/ontology/experiment"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/ITO_substrate">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>ITO substrate</rdfs:label>
    <rdfs:comment>transparent electrode</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://opv-kg.org/ontology/material"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/Open-Circuit_Voltage_(Voc)">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>Open-Circuit Voltage (Voc)</rdfs:label>
    <rdfs:comment>A parameter derived from I-V curves, representing the maximum voltage at open circuit, used to assess interface effects in OPV devices.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://opv-kg.org/ontology/i-v_curve_analysis"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/P3HT_and_PSS_(oxidized_P3HT+)">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>P3HT and PSS (oxidized P3HT+)</rdfs:label>
    <rdfs:comment>A composite material consisting of poly(3-hexylthiophene) (P3HT) and poly(styrenesulfonate) (PSS) in an oxidized state (P3HT+), utilized in the mixed interface layer of organic photovoltaic devices.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://opv-kg.org/ontology/material"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/MorphologicalProperty">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://opv-kg.org/ontology/Property"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/Property">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/Device">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://opv-kg.org/ontology/Material">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
